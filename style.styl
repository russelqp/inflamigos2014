@charset "utf-8"

@import "normalize.styl"

@import "iconos.styl"

@import "style-catalogo.styl"

*
	margin 0
	padding 0

body
	background-color #CCEEFF
	background-repeat repeat-x
	color #FFF
	font-family 'Noto Sans', sans-serif
	font-size 16px
	margin 0 auto
	padding 0

img
	border 0

h1,h2,h3,h4,h5,h6
	margin 0 auto
	font-family 'Wendy One', sans-serif
	text-shadow 1px 1px 1px rgba(60,60,60,0.5)

h1
	color #FC3
	font-size 3.0em
	text-align center

h2
	text-align center

h3
	font-size 2.0em
	padding-left 30px
	color #FC0
	background-color #FE4900
	width 100%
	box-sizing border-box
	text-align left

h4
	color #FFD700
	font-size 1.6em
	padding 2px 10px
	background-color #FE4900
	margin 2px auto 10px auto

h5
	font-size 1.2em
	text-align center

h6
	font-size 1.0em

a
	text-decoration none

section, article, aside, nav, .lista, .lista img, .lista ol, .lista li, .boletin, .boletin p, .menu_inflables ul li, .menu_inflables imghover, .lista-imagenes ul, .lista-imagenes li, #ubicacion
	box-sizing border-box
	margin 2px auto
	list-style-position inside
	list-style-type disc
	color #F30
	padding 0 10px

section
	margin-top 0
	padding 15px
	background rgba(255,255,255,0.1)

h3 img
	float right
	padding-right 20px

header
	display block
	margin 0 auto
	width 100%
	padding 0
	text-align center

header p
	width 80%

header #logo
	border 0
	display inline-block
	width 10%

header #inflabletop
	border 0
	display inline-block
	width 10%

header #info
	display inline-block
	width 78%
	text-align center
	margin 0 auto


header #logo img, header #inflabletop img
	width 100%

header ol
	list-style none
	text-decoration none
	padding 0
	margin 0 auto

article, aside
	display inline-block

article
	color #121212
	margin 0 auto
	padding 0px
	text-align center
	vertical-align top
	width 100%

aside
	width 30%
	margin 0 15px 0 auto
	text-align center
	padding 0px
	color #FFF

 article .lista li p
	width 75%
	margin 5px auto
	text-align justify
	padding 0
	color #39F

article p
	font-size 1.1em
	margin 15px auto
	width 85%

#banner_video
	width 60%
	padding 5px
	margin 0 auto
	display inline-block
	vertical-align top
	background-color #FFF

#boletin_video
	margin 5px auto
	width 100%
	padding 15px
	background none
	border 0
	box-shadow none
	background rgba(255,255,255,0.1)
	text-align center

#servicios
	width 100%
	margin 0 auto
	padding 15px
	background rgba(255,255,255,0.1)
	border-bottom-right-radius 5px
	border-bottom-left-radius 5px

#welcome
	margin 5px auto
	width 100%
	padding 15px
	background none
	border 0
	box-shadow none
	background rgba(255,255,255,0.9)
	text-align center

#welcome #wowslider-container1
		width 67%
		margin 0 auto
		display inline-block
		vertical-align top
		background rgba(255,255,255,0.2)
		position relative

#welcome figure img 
					width 100%
					padding 0
					margin 0


#welcome #box-ofer
		width 30%
		margin 0 auto
		padding 0	
		display inline-block
		vertical-align top

#welcome #box-ofer form
			display block
			margin 0 auto 5px auto
			padding 2px
			height 40%
			width 100%
			text-align center
			background-color #FFD700

#welcome #box-ofer form	input
							border-radius 5px
							border solid 1px #FFF
							margin 2px auto
							height 2em
							width 90%
							transition 0.7s ease all
							color #FE4900
							padding 5px 2px

#welcome #box-ofer form	.user-submit
									box-shadow 0 0 5px #888
									background-color red
									width 80%
									color #eee

#welcome #box-ofer form	.user-submit:hover
							background-color #8EEB00
							color #fff
							box-shadow 0 0 5px #888

#welcome #box-sm
	margin 0 auto
	text-align center
	padding 2px 0
	background rgba(255,255,255,0.7)
	display inline-block
	vertical-align top
	height 40%
	width 100%

#welcome #box-sm ul li
	margin 2px
	padding 0
	display inline-block

#welcome #box-sm ul li a
						display inline-block
						vertical-align top
						padding 0
						font-size 1.8em
						color #D20033
						text-shadow 0 0 2px rgba(60,60,60,0.5)
						transition all 0.5s ease

#welcome #box-sm ul li a:hover
	color #00AF64
	transform scale(1.1)


.miniserv
	border-radius 5px
	border 5px solid #eee
	background-color #fff
	color #333
	padding 10px
	margin 2px 5px
	width 29%
	display inline-block
	vertical-align top
	transition all 1s ease
	text-align center
	box-shadow 0 0 2px 2px rgba(46, 49, 50, 0.3)

	p 
		width 80%
		margin 0
		padding 0
		text-align right

.miniserv .compartir 
		width 40%
		margin 0 2px
		padding 0
		display inline-block
		vertical-align top

.miniserv ul
		padding 0
		list-style-type none
		text-align right

.miniserv ul li 
		display inline-block
		vertical-align top
		padding 0
		width 20%
		margin 0 2px
	
.miniserv ul li a
				display inline-block
				vertical-align top
				padding 0
				font-size 1.8em
				color #D20033
				text-shadow 0 0 2px rgba(60,60,60,0.5)
				transition all 0.5s ease

.miniserv ul li a:hover
				color #00AF64
				transform scale(1.1)

.miniserv img
	width 100%
	margin 0

.miniserv:hover
	transform scale(1.08)

footer
	background-color #FF4900
	bottom 0%
	margin 0 auto
	padding 0
	position relative
	text-align center
	z-index 1
	width 100%

.submenu a
	display block
	text-decoration none
	height 25px
	padding 5px 5px 0 5px
	transition all 0.5s ease
	width 100%

.submenu a:hover
	display block
	background-color #EE3
	width 100%

blockquote
	font-size 18px
	color #FF4933
	font-weight 600
	margin 10px auto
	width 80%
	text-align center
	text-shadow -1px -1px 1px rgba(60,60,60,0.5)

nav
	margin 0 auto
	text-align center
	width 100%
	display block
	padding 0

nav ul
	padding 0
	background-color #FE4900
	font-size  0

nav ul li
	background-color #FF9500
	border 1px solid #D30
	border-radius 2px
	color #FFF
	display inline-block
	font-size 16px
	height 45px
	line-height 45px
	margin 2px
	text-align right
	width 16%

nav ul li a
		display block
		color #FFF
		transition all 1s ease

nav ul li a:hover
	background-color yellow
	text-align center
	color #ee0000
	font-weight: bold

nav ul li a:visited
	border 1px solid #D30

section
	display block
	margin 0 auto
	width 100%
	padding 15px 0
	background-color #EFEFEF

#bk-home,#bk-brincolines,#bk-acercade,#bk-faq,#bk-contacto,#bk-blog
	background-repeat no-repeat
	background-size 25%

#bk-home
	background-image url(img/menu/ico_inicio.png)

#bk-brincolines
	background-image url(img/menu/ico_brincolines.png)

#bk-acercade
	background-image url(img/menu/ico_acercade.png)

#bk-faq
	background-image url(img/menu/ico_faq.png)

#bk-contacto
	background-image url(img/menu/ico_contacto.png)

#bk-blog
	background-image url(img/menu/ico_blog.png)

#mapa
	width 80%
	height 200px
	margin 10px auto
	display block
	border 1px solid #F33
	padding 0

#background
	box-sizing border-box
	height 100%
	margin 0
	padding 0
	position absolute
	top 0%
	width 100%
	z-index 0

#container
	margin 5px auto
	max-width 1024px
	padding 0
	position relative
	text-align justify
	z-index 3

#pasto_agua
	bottom 0%
	height 450px
	margin 0
	padding 0
	position relative
	width 100%
	z-index 1

#contact-area
	background-color #FF8C00
	border-radius 5px
	margin 10px auto
	width 80%
	padding 20px

#contact-area h4
	width 100%
	padding 0
	margin 0


#contact-area p
	font-size 1.4em
	margin 5px auto
	width 90%
	text-align left

#contact-area .nombre, .telefono, .email, textarea
	border 1px solid #CCC
	color #09F
	font-family Helvetica, sans-serif
	font-size 1.6em
	width 100%
	height 1.6em


#contact-area textarea
	font-size 1.4em

#contact-area .opinion
	border 1px dashed #FFF

#contact-area textarea focus, #contact-area input focus
	border 2px solid #F60

#contact-area input.submit-button
	background-color #FE3F1D
	border-radius 5px
	font-family Helvetica, sans-serif
	font-size 1.4em
	font-weight bolder
	height 70px
	margin 0 auto
	text-align center
	width 100%
	border 1px solid #900
	transition 1s all ease

#contact-area input.submit-button:hover
	border 2px solid yellow
	color yellow
	color orange

.btn_activo
	background-color #FFBC00
	box-shadow 1px 1px 2px rgba(60,60,60,0.5)

.submenu a link
	color #09F

.ayudaonline
	width 100%
	height 40px
	margin 0
	color #FC3
	padding 0
	text-align right
	background-image url(imagenes/ayudaonline.png)
	background-position right
	background-repeat no-repeat
	margin-right 20px
	padding-right 50px

.lista
	margin 20px auto
	width 100%
	padding 10px
	text-align center


.lista .img-list, .lista ol
	padding 0
	margin 0
	display inline-block
	vertical-align top

.lista .img-list
	width 15%

.lista .img-list img
	width 100%
	padding 0

.lista ol
	width 75%
	list-style none
	background-color #FC3
	border-radius 5px
	border 2px solid #F90
	padding 10px 0

.lista ol li
		color #f80012
		text-align left
		font-size 1.2em

.ubicacion
	background-color #F90
	margin 0 auto
	padding 0
	width 948px

.ubicacion p
	margin  auto
	text-align left

.uvideo
	margin 10px auto
	padding-top 10px
	text-align center
	width 80%

.uvideo a link
	color #03C

.banner
	width 650px
	margin 0 auto

#social-holder
	width 100%
	margin 5px auto
	text-align center
	padding 2px 0
	background-color #FB0
	border-radius 5px
	display inside-block

#social-holder ul li
	margin 2px
	padding 0
	display inline-block

#social-holder ul li a
	display block
	padding 0
	font-size 1.8em
	color #D20033
	text-shadow 0 0 2px rgba(60,60,60,0.5)
	transition all 0.5s ease

#social-holder ul li a:hover
	color #00AF64
	transform scale(1.1)



.fb-like-box
	margin 10px auto
	text-align center
	background-color #F60
	border 0

.boletin
	background-color #FB0
	border-radius 5px
	border 0
	border 1px solid #FD3
	display inline-block
	margin 0 auto
	padding 5px 2px
	text-align center
	vertical-align top
	width 100%

.boletin p
	width 90%
	background-color #00AF64
	padding 5px
	border-radius 3px
	font-size 1.0em
	color #fff
	margin 5px auto

.boletin .nombre-bol
	width 96%
	margin 0 auto
	padding 0
	height 30px
	border-top 1px solid #09f
	border-right 1px solid #09f

.boletin .email-bol
	width 96%
	margin 0 auto
	padding 2px
	border-top 1px solid #09f
	border-right 1px solid #09f
	color #FFA400

.boletin .btn-bol
	width 90%
	background-color #DDD
	margin 0 auto
	padding 0
	height 35px
	border-radius 5px
	border 1px solid #CCC
	color #555
	box-shadow 0 0 5px rgba(60,60,60,0.5)
	cursor pointer
	transition all 1s ease

.boletin .btn-bol:hover
	background-color #00BF32
	color #fff

.boletin #tlemail
	width 90%
	font-size 1.2em
	color #09f

.submenu
	background-color #FC3
	color #9C0
	padding 0
	text-align center
    margin 0 auto

.submenu ul
	margin 0 auto
	width 80%
	padding 0

.submenu li
	border-left 2px solid #FF9
	border-right 2px solid #F90
	display inline-block
	vertical-align top
	height 30px
	list-style-type none
	margin 0
	padding 0

.submenu li.nb-left
	border-left 0

.submenu li.nb-right
	border-right 0


.credits
	margin 0 auto
	color #FFF
	text-align center


.credits alink
	text-decoration none
	padding 0
	margin-top 0

.credits p
	margin 0 auto
	padding 10px 0
	text-align center
	width 80%


.lista-imagenes
	display inline-block
	vertical-align top
	width 60%
	padding 2%
	text-align center
	margin 0 auto

.lista-imagenes h4
	text-align left
	margin 10px auto

.lista-imagenes ul
	padding 0
	margin 0
	text-align left

.lista-imagenes li
	display inline-block
	list-style-type none
	margin 2px
	transition all 0.8s ease
	width 30%

.lista-imagenes li img
	width 100%
	padding 0
	box-shadow 3px 3px 2px rgba(46, 49, 50, 0.3)
	border 1px solid #D30
	border-radius 5px

.lista-imagenes li:hover
	transform scale(1.08)

.datos
	display inline-block
	vertical-align top
	width 30%
	padding 2%

.menu_inflables_mini
	margin 0 auto
	padding 2%
	width 96%
	background-color #ffb600
	border-radius 5px
	border-bottom-left-radius 0
	border-bottom-right-radius 10px

.menu_inflables_mini li
	list-style none
	margin 0 1px
	padding 0
	text-decoration none
	width 15%
	display inline-block
	text-align center

.menu_inflables_mini li img
	width 100%
	padding 0
	margin 1px
	border 1px dotted #fff

.menu_inflables_mini li img:hover
	opacity: 0.7

.cat
	margin 5px auto
	width 100%

.cat
	margin 0 auto

.articborder
	border-bottom 1px dashed #09F
	margin 10px

#mapadesitio
	margin 0 auto
	width 80%
	padding 0

#mapadesitio li
	color #F60
	float none
	font-size 1.2em
	list-style disc
	margin-left 20px

#mapadesitio a, #mapadesitio a:link, #mapadesitio a:visited
	text-decoration underline

.fb-comments
	width 100%
	text-align center
	margin 0 auto

.fltrt
 	float right
	margin-left 8px

.fltlft
 	float left
	margin-right 8px

.clearfloat
	clear both

@import "responsive.styl"